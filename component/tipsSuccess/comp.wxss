.tips{
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(255, 255, 255, 0.96);
  width: 100%;
  height: 100%;
}
.content{
  top: 50%;
  left: 50%;
  position: absolute;
  width: 140rpx;
  height: 180rpx;
  transform: translateY(-50%) translateX(-50%) ;
  margin-top: -70rpx;
}
.image{
  background: url(https://derucci.net/vip/imgs/vote-check.png) no-repeat center;
  background-size: 100% 100%;
  width: 104rpx;
  height: 104rpx;
  margin: auto;
}
.text{
  font-size: 36rpx;
	line-height: 60rpx;
	color: #333333;
}